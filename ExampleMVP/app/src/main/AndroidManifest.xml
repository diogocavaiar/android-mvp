<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dmc.com.br.examplemvp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App">
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.login.LoginActivity" android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.base.BaseActivity"
            android:label="@string/title_activity_base"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.activity.MainActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".ui.activity.userregister.UserRegister" android:theme="@style/AppTheme"/>
        <activity android:name=".ui.activity.useralter.UserAlter" android:theme="@style/AppTheme"/>

    </application>

</manifest>